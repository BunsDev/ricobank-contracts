{
  "name": "autobank",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "scripts": {
    "init": "git submodule update --init --recursive && npm run init:feedbase && npm run init:gemfab",
    "init:feedbase": "cd lib/feedbase && npm i",
    "init:gemfab": "cd lib/gemfab && npm i",
    "build:all": "npm run build:sol && npm run build:feedbase && npm run build:gemfab",
    "build:sol": "hh compile",
    "build:feedbase": "cd lib/feedbase && npm run build",
    "build:gemfab": "cd lib/gemfab && npm run build",
    "test": "npm run build:sol && ts-mocha --timeout 10000 test/*"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "bigdecimal": "^0.6.1",
    "bn.js": "^4.12.0",
    "chai": "^4.3.4",
    "dpack": "github:dapphub/dpack",
    "hardhat": "^2.6.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
