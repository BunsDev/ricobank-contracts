{
  "name": "autobank",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "scripts": {
    "init": "git submodule update --init --recursive && npm run init:feedbase && npm run init:gemfab",
    "init:feedbase": "cd lib/feedbase && npm i",
    "init:gemfab": "cd lib/gemfab && npm i",
    "build:all": "npm run build:ts && npm run build:sol && npm run build:feedbase && npm run build:gemfab",
    "build:sol": "hh compile",
    "build:ts": "tsc",
    "build:feedbase": "cd lib/feedbase && npm run build",
    "build:gemfab": "cd lib/gemfab && npm run build",
    "pretest": "npm run build:sol",
    "test": "ts-mocha --timeout 10000 test/*",
    "fmt": "ts-standard --fix task test"
  },
  "devDependencies": {
    "@balancer-labs/v2-deployments": "^2.3.0",
    "@defi-wonderland/smock": "^2.0.7",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "bigdecimal": "^0.6.1",
    "bn.js": "^4.12.0",
    "chai": "^4.3.4",
    "hardhat": "^2.6.3",
    "minihat": "^0.0.4",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.4.2",
    "web3-utils": "^1.6.1"
  }
}
